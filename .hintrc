{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}